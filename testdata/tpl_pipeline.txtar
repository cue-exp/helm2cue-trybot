tpl with downstream pipeline functions: the tpl result feeds
into quote normally.

-- input.yaml --
data: {{ tpl .Values.greeting . | quote }}
-- values.yaml --
greeting: hello
-- helm_output.yaml --
data: "hello"
-- output.cue --
import (
	"encoding/yaml"
	"text/template"
)

_tplContext: {
	Capabilities: #capabilities
	Chart:        #chart
	Files:        #files
	Release:      #release
	Template:     #template
	Values:       #values
}

#capabilities: _
#chart:        _
#files:        _
#release:      _
#template:     _
#values: {
	greeting!: bool | number | string | null
	...
}

data: "\(yaml.Unmarshal(template.Execute(#values.greeting, _tplContext)))"
