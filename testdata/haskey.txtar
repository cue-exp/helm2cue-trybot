hasKey function: check if a map contains a key.

-- values.yaml --
config:
  debug: true
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
{{- if hasKey .Values.config "debug" }}
data:
  debug: enabled
{{- end }}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  debug: enabled
-- output.cue --
import "struct"

#values: {
	config?: _
	...
}

apiVersion: "v1"
kind:       "ConfigMap"
metadata: name: "test"
if (_nonzero & {#arg: #values.config.debug, _}) {
	data: debug: "enabled"
}
_nonzero: {
	#arg?: _
	[if #arg != _|_ {
		[
			if (#arg & int) != _|_ {#arg != 0},
			if (#arg & string) != _|_ {#arg != ""},
			if (#arg & float) != _|_ {#arg != 0.0},
			if (#arg & bool) != _|_ {#arg},
			if (#arg & [...]) != _|_ {len(#arg) > 0},
			if (#arg & {...}) != _|_ {(#arg & struct.MaxFields(0)) == _|_},
			false,
		][0]
	}, false][0]
}
