Conditional with nested and/not operators.

-- values.yaml --
name: my-app
enabled: true
maintenance: false
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ .Values.name }}
{{- if and .Values.enabled (not .Values.maintenance) }}
data:
  status: running
{{- end }}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: my-app
data:
  status: running
-- output.cue --
import "struct"

#values: {
	name!:        bool | number | string | null
	enabled?:     bool | number | string | null
	maintenance?: bool | number | string | null
	...
}

apiVersion: "v1"
kind:       "ConfigMap"
metadata: name: #values.name
if (_nonzero & {#arg: #values.enabled, _}) && !(_nonzero & {#arg: #values.maintenance, _}) {
	data: status: "running"
}
_nonzero: {
	#arg?: _
	[if #arg != _|_ {
		[
			if (#arg & int) != _|_ {#arg != 0},
			if (#arg & string) != _|_ {#arg != ""},
			if (#arg & float) != _|_ {#arg != 0.0},
			if (#arg & bool) != _|_ {#arg},
			if (#arg & [...]) != _|_ {len(#arg) > 0},
			if (#arg & {...}) != _|_ {(#arg & struct.MaxFields(0)) == _|_},
			false,
		][0]
	}, false][0]
}
