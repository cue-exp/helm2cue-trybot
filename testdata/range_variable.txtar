Range over a local variable assigned from .Values.

-- values.yaml --
items:
  - alpha
  - beta
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  {{- $items := .Values.items }}
  items:
    {{- range $items }}
    - {{ . }}
    {{- end }}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  items:
    - alpha
    - beta
-- output.cue --
#values: {
	items!: _
	...
}

output: [
	{
		apiVersion: "v1"
		kind:       "ConfigMap"
		metadata: name: "test"
		data: items: [
			for _, _range0 in #values.items {
				_range0
			},
		]
	},
]
