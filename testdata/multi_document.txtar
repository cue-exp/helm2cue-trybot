Multi-document YAML template with --- separator.

-- _helpers.tpl --
-- values.yaml --
name: test
-- input.yaml --
---
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ .Values.name }}
data:
  key: one
---
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ .Values.name }}
data:
  key: two
-- output.cue --
#values: {
	name!: bool | number | string | null
	...
}

document_0: {
	apiVersion: "v1"
	kind:       "ConfigMap"
	metadata: name: #values.name
	data: key: "one"
}
document_1: {
	apiVersion: "v1"
	kind:       "ConfigMap"
	metadata: name: #values.name
	data: key: "two"
}
