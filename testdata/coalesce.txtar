coalesce function: return first non-empty value.

-- values.yaml --
a: ""
b: fallback
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  result: {{ coalesce .Values.a .Values.b "default" }}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: test
data:
  result: fallback
-- output.cue --
import "struct"

#values: {
	a!: bool | number | string | null
	b!: bool | number | string | null
	...
}

apiVersion: "v1"
kind:       "ConfigMap"
metadata: name: "test"
data: result: [if (_nonzero & {#arg: #values.a, _}) {#values.a}, if (_nonzero & {#arg: #values.b, _}) {#values.b}, "default"][0]
_nonzero: {
	#arg?: _
	[if #arg != _|_ {
		[
			if (#arg & int) != _|_ {#arg != 0},
			if (#arg & string) != _|_ {#arg != ""},
			if (#arg & float) != _|_ {#arg != 0.0},
			if (#arg & bool) != _|_ {#arg},
			if (#arg & [...]) != _|_ {len(#arg) > 0},
			if (#arg & {...}) != _|_ {(#arg & struct.MaxFields(0)) == _|_},
			false,
		][0]
	}, false][0]
}
