Conditional with not operator.

-- values.yaml --
name: my-app
debug: false
-- input.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ .Values.name }}
{{- if not .Values.debug }}
data:
  env: production
{{- end }}
-- helm_output.yaml --
apiVersion: v1
kind: ConfigMap
metadata:
  name: my-app
data:
  env: production
-- output.cue --
import "struct"

#values: {
	name!:  bool | number | string | null
	debug?: bool | number | string | null
	...
}

apiVersion: "v1"
kind:       "ConfigMap"
metadata: name: #values.name
if !(_nonzero & {#arg: #values.debug, _}) {
	data: env: "production"
}
_nonzero: {
	#arg?: _
	[if #arg != _|_ {
		[
			if (#arg & int) != _|_ {#arg != 0},
			if (#arg & string) != _|_ {#arg != ""},
			if (#arg & float) != _|_ {#arg != 0.0},
			if (#arg & bool) != _|_ {#arg},
			if (#arg & [...]) != _|_ {len(#arg) > 0},
			if (#arg & {...}) != _|_ {(#arg & struct.MaxFields(0)) == _|_},
			false,
		][0]
	}, false][0]
}
